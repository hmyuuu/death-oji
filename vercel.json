{
  "buildCommand": "cd frontend && bun run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && bun install",
  "framework": "astro",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-backend-url.railway.app/api/:path*"
    },
    {
      "source": "/health",
      "destination": "https://your-backend-url.railway.app/health"
    }
  ]
}
